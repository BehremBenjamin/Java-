public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        StringBuffer B = new StringBuffer(A).reverse();
        String C = B.toString();   
        if(C.equals(A)) System.out.println("Yes"); 
        else System.out.println("No");
    }
}
